
{
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "DocumentFragment": true,
        "_": true,
        "escape": true,
        "gapi": true,
        "process": true,
        "module": true,
        "__dirname": true

    },
    "rules": {
        "camelcase": 1,
        "valid-jsdoc": 0,
        "eqeqeq": 0,
        "curly": ["multi"],
        "brace-style": [1, "stroustrup"],
        "consistent-this": 2,
        "no-constant-condition": 1,
        "no-underscore-dangle": 0,
        "no-use-before-define": 0,
        "func-names": 0,
        "func-style": [2, "declaration"],
        "new-cap": 1,
        "new-parens": 2,
        "no-ternary": 0,
        "no-unused-vars": [1, {"vars": "local", "args": "none"}],
        "quotes": [2, "single"],
        "one-var": 0,
        "space-infix-ops": 0,
        "strict": 1,
        "no-global-strict": 0
    }
}
